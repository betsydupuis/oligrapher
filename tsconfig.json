{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": false,
    "module": "es2020",
    "target": "es5",
    "moduleResolution": "node",
    "jsx": "react",
    "lib": ["es2020", "dom"],
    "noImplicitThis": true,
    "strictNullChecks": true,
    "esModuleInterop": true
  },
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  }
}
