{
  "name": "@public-accountability/oligrapher",
  "version": "3.2.0",
  "description": "Network graph visualizer built for LittleSis",
  "main": "app/Oligrapher.tsx",
  "scripts": {
    "build-dev": "webpack --color --progress",
    "build-dev-one": "webpack --color --progress --env onefile",
    "build-prod": "webpack --color --progress --env production",
    "build-prod-one": "webpack --color --progress --env production --env onefile",
    "dev-server": "webpack serve --color --progress --env dev_server",
    "build": "npm run build-prod && npm run build-prod-one",
    "test": "jest",
    "lint": "npm run eslint ./app --ext .js,.jsx,.ts,.tsx",
    "stats": "webpack --env production --profile --json > stats.json",
    "analyze": "webpack-bundle-analyzer stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/public-accountability/oligrapher"
  },
  "author": "Public Accountability Initiative",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/public-accountability/oligrapher/issues"
  },
  "homepage": "https://littlesis.org/oligrapher",
  "browserslist": [
    "last 2 chrome versions",
    "last 2 firefox versions",
    "last 1 edge versions",
    "last 1 safari versions"
  ],
  "dependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@ckeditor/ckeditor5-build-classic": "^35.0.1",
    "@ckeditor/ckeditor5-react": "^5.0.2",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@hello-pangea/dnd": "^16.0.0",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/component": "^5.15.2",
    "@mui/material": "^5.10.2",
    "@react-icons/all-files": "^4.1.0",
    "@reduxjs/toolkit": "^1.8.5",
    "@types/lodash": "^4.14.184",
    "@types/redux-logger": "^3.0.9",
    "babel-loader": "^8.2.5",
    "core-js": "^3.25.0",
    "css-loader": "^6.7.1",
    "git-revision-webpack-plugin": "^5.0.0",
    "immer": "^9.0.15",
    "jsdom": "^20.0.0",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-hotkeys-hook": "^3.4.7",
    "react-redux": "^8.0.2",
    "react-resize-detector": "^7.1.2",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.1",
    "redux-undo": "^1.0.1",
    "sass": "^1.54.5",
    "sass-loader": "^13.0.2",
    "slugify": "^1.6.5",
    "springy": "^2.8.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0",
    "wretch": "^2.0.4"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "chai": "^4.3.6",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "msw": "^0.45.0",
    "react-test-renderer": "^18.2.0",
    "redux-saga-test-plan": "^4.0.5"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.[jt]s?(x)"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(nanoid|lodash-es)/)"
    ],
    "testEnvironment": "jsdom"
  }
}
