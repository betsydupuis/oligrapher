{
  "name": "@public-accountability/oligrapher",
  "version": "3.1.0",
  "description": "Network graph visualizer built for LittleSis",
  "main": "app/Oligrapher.jsx",
  "scripts": {
    "build-dev": "webpack --color --progress",
    "build-dev-one": "webpack --color --progress --env onefile",
    "build-prod": "webpack --color --progress --env production",
    "build-prod-one": "webpack --color --progress --env production --env onefile",
    "dev-server": "webpack serve --color --progress --env dev_server",
    "build": "npm run build-prod && yarn run build-prod-one",
    "test": "mocha --exit",
    "lint": "npm run eslint ./app --ext .js,.jsx,.ts,.tsx",
    "stats": "webpack --env production --profile --json > stats.json",
    "analyze": "webpack-bundle-analyzer stats.json"
  },
